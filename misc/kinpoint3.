     vel=dsqrt(1d0-m_top**2/EHat**2)
     parx=2d0*EHat/(1d0+dsqrt(2d0)+dsqrt(3d0))

     Mom(1,4) = (EHat)
     Mom(2,4) = (0d0)
     Mom(3,4) = (0d0)
     Mom(4,4) = (vel*EHat)

     Mom(1,5) = (EHat)
     Mom(2,5) = (0d0)
     Mom(3,5) = (0d0)
     Mom(4,5) = (-vel*EHat)

     Mom(1,1) = -(-parx)
     Mom(2,1)=  -(parx)
     Mom(3,1) = -(0d0)
     Mom(4,1) = -(0d0)

     Mom(1,2) = -(-dsqrt(2d0)*parx)
     Mom(2,2) = -(0d0)
     Mom(3,2) = -(parx)
     Mom(4,2) = -(parx)

     Mom(1,3) = (-dsqrt(3d0)*parx)
     Mom(2,3) = (-parx)
     Mom(3,3) = (-parx)
     Mom(4,3) = (-parx)

